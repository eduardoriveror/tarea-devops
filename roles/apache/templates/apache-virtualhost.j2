<VirtualHost *:8080>

        DocumentRoot {{ nginx_directory }}/tarea
        <Directory {{ nginx_directory }}/tarea>
            Require all granted
            AllowOverride All
        </Directory>

        ErrorLog ${APACHE_LOG_DIR}/error.log
        CustomLog ${APACHE_LOG_DIR}/access.log combined

</VirtualHost>

